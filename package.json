{
    "name": "@freebot/openbot",
    "publishConfig": {
        "access": "public"
    },
    "version": "1.0.0",
    "description": "CLI and library to run prompts with skill paths (Agent Skills style). Use as npm package or freebot CLI.",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        }
    },
    "files": [
        "dist",
        "skills",
        "README.md"
    ],
    "bin": {
        "freebot": "./dist/cli.js"
    },
    "dependencies": {
        "@mariozechner/pi-ai": "^0.51.2",
        "@mariozechner/pi-coding-agent": "^0.51.2",
        "@sinclair/typebox": "^0.34.41",
        "agent-browser": "^0.8.5",
        "commander": "^14.0.2",
        "ws": "^8.19.0",
        "@nestjs/common": "^10.3.0",
        "@nestjs/core": "^10.3.0",
        "@nestjs/platform-express": "^10.3.0",
        "@nestjs/platform-socket.io": "^10.3.0",
        "@nestjs/websockets": "^10.3.0",
        "reflect-metadata": "^0.2.1",
        "rxjs": "^7.8.1"
    },
    "devDependencies": {
        "@types/node": "^22.0.0",
        "@types/ws": "^8.18.1",
        "@types/express": "^4.17.21",
        "typescript": "^5.7.0"
    },
    "scripts": {
        "clean": "rm -rf dist",
        "build": "npm run clean && tsc",
        "start": "node dist/cli.js",
        "run": "node dist/cli.js",
        "dev": "npm run build && node dist/cli.js",
        "desktop:dev": "cd desktop && npm run dev",
        "desktop:build": "cd desktop && npm run build",
        "desktop:install": "cd desktop && npm install"
    },
    "engines": {
        "node": ">=20.0.0"
    },
    "keywords": [
        "skill",
        "agent",
        "cli",
        "llm",
        "freebot"
    ],
    "license": "MIT"
}